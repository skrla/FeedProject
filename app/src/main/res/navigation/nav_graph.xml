<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/testFragment">

    <fragment
        android:id="@+id/feedFragment"
        android:name="skrla.feedprocject.ui.fragments.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_feedFragment_to_athleteFragment"
            app:destination="@id/athleteFragment" />
    </fragment>
    <fragment
        android:id="@+id/testFragment"
        android:name="skrla.feedprocject.ui.fragments.TestFragment"
        android:label="fragment_test"
        tools:layout="@layout/fragment_test" >
        <action
            android:id="@+id/action_testFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
        <action
            android:id="@+id/action_testFragment_to_athleteFragment"
            app:destination="@id/athleteFragment" />
        <action
            android:id="@+id/action_testFragment_to_singleAthleteFragment"
            app:destination="@id/singleAthleteFragment" />
    </fragment>
    <fragment
        android:id="@+id/athleteFragment"
        android:name="skrla.feedprocject.ui.fragments.AthleteFragment"
        android:label="fragment_athlete"
        tools:layout="@layout/fragment_athlete" />
    <fragment
        android:id="@+id/singleAthleteFragment"
        android:name="skrla.feedprocject.ui.fragments.SingleAthleteFragment"
        android:label="fragment_single_athlete"
        tools:layout="@layout/fragment_single_athlete" />
</navigation>